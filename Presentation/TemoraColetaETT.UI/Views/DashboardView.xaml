<?xml version="1.0" encoding="utf-8" ?><ContentPage     x:Class="TemoraColetaETT.UI.Views.DashboardView"    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"    x:Name="ThisPage"    Title="Dashboard">
    <ContentPage.Behaviors>        <toolkit:EventToCommandBehavior            EventName="Appearing"            Command="{Binding LoadDashboardCommand}" />    </ContentPage.Behaviors>    <VerticalStackLayout>        <Label Text="{Binding WelcomeMessage}"                HorizontalOptions="Center"                VerticalOptions="Center" />    </VerticalStackLayout></ContentPage>